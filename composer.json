{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 9 projects with Composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.4",
        "aws/aws-sdk-php": "^3.225",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/auto_username": "1.4",
        "drupal/console": "~1.0",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/d8w3css": "^2.0",
        "drupal/entitygroupfield": "^1.0@alpha",
        "drupal/gin": "^3.0@beta",
        "drupal/group": "1.4",
        "drupal/mail_login": "2.7",
        "drupal/pathauto": "1.9",
        "drupal/publishcontent": "^1.4",
        "drupal/queue_monitor": "2.0.*",
        "drupal/restui": "^1.20",
        "drupal/s3fs": "^3.0@beta",
        "drupal/seckit": "2.0",
        "drupal/single_content_sync": "1.3",
        "drupal/split_preview": "^1.0",
        "drupal/subgroup": "1.0",
        "drupal/twig_tweak": "^3.1",
        "drupal/view_unpublished": "^1.0",
        "drupal/views_data_export": "1.1",
        "drupal/workflow": "1.5",
        "drush/drush": "^10.6",
        "firebase/php-jwt": "^6.8",
        "opensearch-project/opensearch-php": "^2.0",
        "php-webdriver/webdriver": "^1.12",
        "phpmailer/phpmailer": "^6.6",
        "vlucas/phpdotenv": "^5.1",
        "webflo/drupal-finder": "^1.2"
    },
    "require-dev": {
        "drupal/core-dev": "^9.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/console-extend-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/index.php": false,
                "[web-root]/sites/development.services.yml": false,
                "[web-root]/.htaccess": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
        }
    }
}
